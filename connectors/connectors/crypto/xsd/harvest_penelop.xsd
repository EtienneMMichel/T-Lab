<xs:schema attributeFormDefault="unqualified" elementFormDefault="qualified" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:annotation id="PENELOP_1">
		<xs:documentation>id=1: V1.22.1 Version de production au 21/07/2022 Copyright AGAP 2022 tous droits réservés pour tout renseignement https://penelop.org/</xs:documentation>
	</xs:annotation>
	<xs:element name="penelop">
		<xs:annotation id="PENELOP_11">
			<xs:documentation>id=11: élement racine d'un fichier PENELOP</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element minOccurs="0" name="donnee_reguliere">
					<xs:annotation id="PENELOP_21">
						<xs:documentation>id=21: Ensemble des données fournies à intervalle régulier (quotidienne, hebdomadaire,mensuelle,...)</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence maxOccurs="unbounded">
							<xs:element name="destinataire">
								<xs:annotation id="PENELOP_31">
									<xs:documentation>id=31: un fichier PENELOP peut potentiellement regrouper des données destinées à plusieurs intermédiaires, cas par exemple de fichier étant fourni à un agrégateur qui répartira les données pour chaque destinataire final</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:all>
										<!-- 		modif 20140212 mantis#247 pour permettre d'envoyer des fichiers sans qu'aucun contrat ou client ne soit en place-->
										<xs:element minOccurs="0" name="personnes">
											<xs:annotation id="PENELOP_41">
												<xs:documentation>id=41: Collection des personnes concernées par les contrats</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence maxOccurs="unbounded">
													<!-- modif 20170901 mantis#330 : minoccurs mis à zéro-->
													<xs:element minOccurs="0" name="personne" type="personne_type">
														<xs:annotation id="PENELOP_51">
															<xs:documentation>id=51: Une personne physique ou morale</xs:documentation>
														</xs:annotation>
													</xs:element>
													<!-- debut ajout 20170109 mantis#298-->
													<xs:element name="anomalies" type="anomalies_type" minOccurs="0">
														<xs:annotation id="PENELOP_11721">
															<xs:documentation>id=11721: collection des anomalies de 'personne' (données régulières) détectées à la production du fichier</xs:documentation>
														</xs:annotation>
													</xs:element>
													<!-- fin ajout 20170109 mantis#298-->
													<!-- debut ajout 20170706 mantis#320-->
													<!-- debut modif 20190729 mantis#355-->
													<xs:element name="dsf_ajouts" type="dr.personnes.dsf_ajouts_type" minOccurs="0">
														<xs:annotation id="PENELOP_12361">
															<xs:documentation>id=12361: collection de données hors schéma</xs:documentation>
														</xs:annotation>
													</xs:element>
													<!-- fin modif 20190729 mantis#355-->
													<!-- fin ajout 20170706 mantis#320-->
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<xs:element minOccurs="0" name="contrats">
											<xs:annotation id="PENELOP_61">
												<xs:documentation>id=61: Collection des contrats gérés par l'organisme destinataire</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence maxOccurs="unbounded">
													<xs:element minOccurs="0" name="contrat" type="contrat_donnees_regulieres_type">
														<xs:annotation id="PENELOP_71">
															<xs:documentation>id=71: un contrat (ou compte)</xs:documentation>
														</xs:annotation>
														<!-- debut ajout 20120116 mantis#180 -->
														<xs:key name="evt_global_contrat">
															<xs:annotation id="PENELOP_10091">
																<xs:documentation>id=10091:unicité de l'ID Evt Global au sein d'un même contrat. On la teste à code_statut_evt constant, car  l'ID peut être répété dans un mouvement de correction</xs:documentation>
															</xs:annotation>
															<!-- debut modif 20220614 mantis#433 -->
															<!-- <xs:selector xpath="evt_globaux/evt_global"/> -->
															<xs:selector xpath=".//evt_globaux/evt_global"/>
															<!-- fin modif 20220614 mantis#433 -->
															<xs:field xpath="@ID_evt_global"/>
															<xs:field xpath="@code_statut_evt"/>
														</xs:key>
														<!-- fin ajout 20120116 mantis#180 -->
														<!-- debut ajout 20120119 mantis#184 -->
														<xs:key name="support_composant_contrat_DR">
															<xs:annotation id="PENELOP_10111">
																<xs:documentation>id=10111:unicité de l'ID-support_composant au sein d'un même contrat</xs:documentation>
															</xs:annotation>
															<!-- debut modif 20220614 mantis#433 -->
															<!-- <xs:selector xpath="support_composants/support_composant"/> -->
															<xs:selector xpath=".//support_composants/support_composant"/>
															<!-- fin modif 20220614 mantis#433 -->
															<xs:field xpath="@ID_support_composant"/>
														</xs:key>
														<!-- fin ajout 20120119 mantis#184 -->
													</xs:element>
													<!-- debut ajout 20170109 mantis#298-->
													<xs:element minOccurs="0" name="anomalies" type="anomalies_type">
														<xs:annotation id="PENELOP_11731">
															<xs:documentation>id=11731: collection des anomalies de 'contrat' (données régulières) détectées à la production du fichier</xs:documentation>
														</xs:annotation>
													</xs:element>
													<!-- fin ajout 20170109 mantis#298-->
													<!-- debut ajout 20190816 mantis#320-->
													<xs:element name="dsf_ajouts" type="dr.contrats.dsf_ajouts_type" minOccurs="0">
														<xs:annotation id="PENELOP_13711">
															<xs:documentation>id=13711: collection de données hors schéma</xs:documentation>
														</xs:annotation>
													</xs:element>
													<!-- fin ajout 20190816 mantis#320-->
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<!-- debut ajout 20170704 mantis#305 -->
										<xs:element minOccurs="0" name="credits">
											<xs:complexType>
												<xs:sequence maxOccurs="unbounded">
													<xs:element minOccurs="0" name="credit" type="credit_type">
														<xs:annotation id="PENELOP_12431">
															<xs:documentation>id=12431: collection des crédits</xs:documentation>
														</xs:annotation>
													</xs:element>
													<xs:element minOccurs="0" name="anomalies" type="anomalies_type">
														<xs:annotation id="PENELOP_12691">
															<xs:documentation>id=12691: collection des anomalies de 'credit' (données régulières) détectées à la production du fichier</xs:documentation>
														</xs:annotation>
													</xs:element>
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<!-- fin ajout 20170704 mantis#305 -->
										<!-- debut ajout 20170706 mantis#320-->
										<!-- debut modif 20190729 mantis#355-->
										<xs:element name="dsf_ajouts" type="dr.destinataire.dsf_ajouts_type" minOccurs="0">
											<xs:annotation id="PENELOP_12371">
												<xs:documentation>id=12371: collection de données hors schéma</xs:documentation>
											</xs:annotation>
										</xs:element>
										<!-- fin modif 20190729 mantis#355-->
										<!-- fin ajout 20170706 mantis#320-->
									</xs:all>
									<xs:attribute name="ref_ID_intermediaire" type="ID_penelop" use="required">
										<xs:annotation id="PENELOP_81">
											<xs:documentation>id=81: identifiant de l'intermédiaire de PREMIER NIVEAU (entité juridique) concernée par les données</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="date_debut_historique_fichier" type="date_type" use="required">
										<xs:annotation id="PENELOP_91">
											<xs:documentation>id=91: date de début d'historique des évènements fournis dans les données régulières: c'est la date utilisée comme critère de l'extraction des mouvements, qu'il y ait des mouvements dans la période ou non. La borne est incluse</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="date_fin_historique_fichier" type="date_type" use="required">
										<xs:annotation id="PENELOP_101">
											<xs:documentation>id=101: date de fin d'historique des évènements fournis dans les données dans les données régulières: c'est la date utilisée comme critère de l'extraction des mouvements, qu'il y ait des mouvements dans la période ou non. La borne est incluse</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="info_complementaire" type="chaine_longue_type" use="optional">
										<xs:annotation id="PENELOP_6281">
											<xs:documentation>id=6281: information complémentaire niveau destinataire des données regulières</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
					<!-- debut ajout 20110518 mantis#148 -->
					<xs:key name="destinataire_reg">
						<xs:annotation id="PENELOP_9911">
							<xs:documentation>id=9911:unicité destinataire (données régulières): un seul noeud "destinataire" pour un même ref_ID_intermediaire</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire"/>
						<xs:field xpath="@ref_ID_intermediaire"/>
					</xs:key>
					<!-- fin ajout 20110518 mantis#148 -->
					<!-- debut modif 20170117 mantis#313 -->
					<xs:key name="contrat_reg">
						<xs:annotation id="PENELOP_9871">
							<xs:documentation>id=9871: Unicité du couple (ID contrat,num contrat) en données régulières</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/contrats/contrat"/>
						<xs:field xpath="@ID_contrat"/>
						<xs:field xpath="@num_contrat"/>
					</xs:key>
					<!-- fin modif 20170117 mantis#313 -->
					<!-- debut ajout 20140212 mantis#245 -->
					<!-- 		+ correction 20140305 mantis#245 -->
					<xs:unique name="unique_ref_personne_reg">
						<xs:annotation id="PENELOP_10621">
							<xs:documentation>id=10621: Unicité de personne/@ref_personne (identifiant personne interne au fournisseur) en données régulières</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/personnes/personne"/>
						<xs:field xpath="@ref_personne"/>
					</xs:unique>
					<!-- fin ajout 20140212 mantis#245 -->
					<xs:key name="personne_reg">
						<xs:annotation id="PENELOP_6551">
							<xs:documentation>id=6551: association personne - role : clé sur la personne (ID_personne)</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/personnes/personne"/>
						<xs:field xpath="@ID_personne"/>
					</xs:key>
					
					<!-- debut ajout 20220616 mantis#428 -->
					<xs:keyref name="ref_personne_representant_reg" refer="personne_reg">
						<xs:annotation id="PENELOP_16951">
							<xs:documentation>id=16951:  association personne - representant : clé externe depuis le representé vers le representant</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/personnes/personne/representants/representant"/>
						<xs:field xpath="@ref_ID_personne"/>
					</xs:keyref>
					<!-- fin ajout 20220616 mantis#428 -->					
					
					
					
					<xs:keyref name="ref_personne_reg" refer="personne_reg">
						<xs:annotation id="PENELOP_6561">
							<xs:documentation>id=6561:  association personne - role : clé externe depuis le role contrat</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/contrats/contrat/roles/role"/>
						<xs:field xpath="@ref_ID_personne"/>
					</xs:keyref>
					<!-- debut ajout 20170704 mantis#305 -->
					<xs:keyref name="ref_personne_credit" refer="personne_reg">
						<xs:annotation id="PENELOP_12441">
							<xs:documentation>id=12441:  association personne - role : clé externe depuis le role crédit</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/credits/credit/roles/role"/>
						<xs:field xpath="@ref_ID_personne"/>
					</xs:keyref>
					<!-- fin ajout 20170704 mantis#305 -->
					<!-- debut ajout 20170703 mantis#315 -->
					<xs:key name="condition_reg">
						<xs:annotation id="PENELOP_12531">
							<xs:documentation>id=12531:  association condition_contrat - evt_global : clé sur la condition_contrat (ID_condition_contrat)</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/contrats/contrat/conditions_contrat/condition_contrat"/>
						<xs:field xpath="@ID_condition_contrat"/>
					</xs:key>
					<xs:keyref name="ref_condition_reg" refer="condition_reg">
						<xs:annotation id="PENELOP_12541">
							<xs:documentation>id=12541:  association condition_contrat - evt_global : clé externe depuis l'evt_global</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/contrats/contrat/evt_globaux/evt_global"/>
						<xs:field xpath="@ref_ID_condition_contrat"/>
					</xs:keyref>
					<xs:key name="service_reg">
						<xs:annotation id="PENELOP_12551">
							<xs:documentation>id=12551:  association service_contrat - evt_global : clé sur le service_contrat (ID_service_contrat)</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/contrats/contrat/services_contrat/service_contrat"/>
						<xs:field xpath="@ID_service_contrat"/>
					</xs:key>
					<xs:keyref name="ref_service_reg" refer="service_reg">
						<xs:annotation id="PENELOP_12561">
							<xs:documentation>id=12561:  association service_contrat - evt_global : clé externe depuis l'evt_global</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/contrats/contrat/evt_globaux/evt_global"/>
						<xs:field xpath="@ref_ID_service_contrat"/>
					</xs:keyref>
					<!-- fin ajout 20170703 mantis#315 -->
				</xs:element>
				<xs:element minOccurs="0" name="situation_fin_d_annee">
					<xs:annotation id="PENELOP_111">
						<xs:documentation>id=111: Ensemble des données représentant la situation des contrats au 31 décembre de l'année précédente. Est généralement fourni au cours du 1er trimestre de l'année suivant le décompte, et pendant une période déterminée au gré à gré entre fournisseur et destinataire (1 fois, durant 1 semaine,...)</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence maxOccurs="unbounded">
							<xs:element name="destinataire">
								<xs:annotation id="PENELOP_121">
									<xs:documentation>id=121:</xs:documentation>
								</xs:annotation>
								<xs:complexType>
									<xs:all>
										<!-- 		modif 20140212 mantis#247 pour permettre d'envoyer des fichiers sans qu'aucun contrat ou client ne soit en place-->
										<xs:element minOccurs="0" name="personnes">
											<xs:annotation id="PENELOP_131">
												<xs:documentation>id=131:</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence maxOccurs="unbounded">
													<xs:element name="personne" type="personne_type">
														<xs:annotation id="PENELOP_141">
															<xs:documentation>id=141:</xs:documentation>
														</xs:annotation>
													</xs:element>
													<!-- debut ajout 20170109 mantis#298-->
													<xs:element name="anomalies" type="anomalies_type" minOccurs="0">
														<xs:annotation id="PENELOP_11701">
															<xs:documentation>id=11701: collection des anomalies de 'personne' (données fin d'année) détectées à la production du fichier</xs:documentation>
														</xs:annotation>
													</xs:element>
													<!-- fin ajout 20170109 mantis#298-->
													<!-- debut ajout 20170706 mantis#320-->
													<!-- debut modif 20190729 mantis#355-->
													<xs:element name="dsf_ajouts" type="sf.personnes.dsf_ajouts_type" minOccurs="0">
														<xs:annotation id="PENELOP_12381">
															<xs:documentation>id=12381: collection de données hors schéma</xs:documentation>
														</xs:annotation>
													</xs:element>
													<!-- fin modif 20190729 mantis#355-->
													<!-- fin ajout 20170706 mantis#320-->
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<!-- 		modif 20140212 mantis#247 minOccurs="0" pour permettre d'envoyer des fichiers sans qu'aucun contrat ou client ne soit en place-->
										<xs:element minOccurs="0" name="contrats">
											<xs:annotation id="PENELOP_151">
												<xs:documentation>id=151:</xs:documentation>
											</xs:annotation>
											<xs:complexType>
												<xs:sequence maxOccurs="unbounded">
													<xs:element name="contrat" type="contrat_donnees_fin_dannee_type">
														<xs:annotation id="PENELOP_161">
															<xs:documentation>id=161: les images de contrat en fin d'année ne contiennent pas d'évènements</xs:documentation>
														</xs:annotation>
														<!-- debut ajout 20120119 mantis#184 -->
														<xs:key name="support_composant_contrat_SF">
															<xs:annotation id="PENELOP_10121">
																<xs:documentation>id=10121:unicité de l'ID-support_composant au sein d'un même contrat</xs:documentation>
															</xs:annotation>
															<!-- debut modif 20220614 mantis#433 -->
															<!-- <xs:selector xpath="support_composants/support_composant"/> -->
															<xs:selector xpath=".//support_composants/support_composant"/>
															<!-- fin modif 20220614 mantis#433 -->
															<xs:field xpath="@ID_support_composant"/>
														</xs:key>
														<!-- fin ajout 20120119 mantis#184 -->
													</xs:element>
													<!-- debut ajout 20170109 mantis#298-->
													<xs:element name="anomalies" type="anomalies_type" minOccurs="0">
														<xs:annotation id="PENELOP_11711">
															<xs:documentation>id=11711: collection des anomalies de 'contrat' (données fin d'année) détectées à la production du fichier</xs:documentation>
														</xs:annotation>
													</xs:element>
													<!-- fin ajout 20170109 mantis#298-->
													<!-- debut ajout 20170706 mantis#320-->
													<!-- debut modif 20190729 mantis#355-->
													<xs:element name="dsf_ajouts" type="sf.contrats.dsf_ajouts_type" minOccurs="0">
														<xs:annotation id="PENELOP_12391">
															<xs:documentation>id=12391: collection de données hors schéma</xs:documentation>
														</xs:annotation>
													</xs:element>
													<!-- fin modif 20190729 mantis#355-->
													<!-- fin ajout 20170706 mantis#320-->
												</xs:sequence>
											</xs:complexType>
										</xs:element>
										<!-- debut ajout 20190816 mantis#355-->
										<xs:element name="dsf_ajouts" type="sf.destinataire.dsf_ajouts_type" minOccurs="0">
											<xs:annotation id="PENELOP_13731">
												<xs:documentation>id=13731: collection de données hors schéma</xs:documentation>
											</xs:annotation>
										</xs:element>
										<!-- fin ajout 20190816 mantis#355-->
									</xs:all>
									<xs:attribute name="ref_ID_intermediaire" type="ID_penelop" use="required">
										<xs:annotation id="PENELOP_171">
											<xs:documentation>id=171: identifiant complet de l'intermédiaire concerné (niveau de rupture du bordereau)</xs:documentation>
										</xs:annotation>
									</xs:attribute>
									<xs:attribute name="info_complementaire" type="chaine_longue_type" use="optional">
										<xs:annotation id="PENELOP_6291">
											<xs:documentation>id=6291: informations complémentaires niveau destinataire des données de situation fin d'année</xs:documentation>
										</xs:annotation>
									</xs:attribute>
								</xs:complexType>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
					<!-- debut ajout 20110518 mantis#148 -->
					<xs:key name="destinataire_reg_findannee">
						<xs:annotation id="PENELOP_9921">
							<xs:documentation>id=9921: unicité destinataire (données fin d'année): un seul noeud "destinataire" pour un même ref_ID_intermediaire</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire"/>
						<xs:field xpath="@ref_ID_intermediaire"/>
					</xs:key>
					<!-- fin ajout 20110518 mantis#148 -->
					<!-- debut modif 20170117 mantis#313 -->
					<xs:key name="contrat_findannee">
						<xs:annotation id="PENELOP_9891">
							<xs:documentation>id=9891: Unicité du couple (ID contrat,num contrat) en données fin d'année</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/contrats/contrat"/>
						<xs:field xpath="@ID_contrat"/>
						<xs:field xpath="@num_contrat"/>
					</xs:key>
					<!-- fin modif 20170117 mantis#313 -->
					<!-- debut ajout 20140212 mantis#245 -->
					<!-- 		+ correction 20140305 mantis#245 -->
					<xs:unique name="unique_ref_personne_findannee">
						<xs:annotation id="PENELOP_10631">
							<xs:documentation>id=10631: Unicité de personne/@ref_personne (identifiant personne interne au fournisseur)  en données fin d'année</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/personnes/personne"/>
						<xs:field xpath="@ref_personne"/>
					</xs:unique>
					<!-- fin ajout 20140212 mantis#245 -->
					<xs:key name="personne_findannee">
						<xs:annotation id="PENELOP_6571">
							<xs:documentation>id=6571: association personne - role : clé sur la personne (ID_personne)</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/personnes/personne"/>
						<xs:field xpath="@ID_personne"/>
					</xs:key>
					<!-- debut ajout 20220616 mantis#428 -->
					<xs:keyref name="ref_personne_representant_findannee" refer="personne_findannee">
						<xs:annotation id="PENELOP_16961">
							<xs:documentation>id=16961:  association personne - representant : clé externe depuis le representé vers le representant</xs:documentation>
						</xs:annotation>
						<xs:selector xpath="destinataire/personnes/personne/representants/representant"/>
						<xs:field xpath="@ref_ID_personne"/>
					</xs:keyref>
					<!-- fin ajout 20220616 mantis#428 -->	
					
					<xs:keyref name="ref_personne_findannee" refer="personne_findannee">
						<xs:annotation id="PENELOP_6581">
